{
    "manifest_version": 2,

    "name": "Unizar Moodle2 Orange Fix",
    "description": "Kill that orange thing!!",
    "version": "1.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Fix that orange thing!",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "css": [],
        "js": ["fixOrange.js"],
        "matches": ["https://moodle2.unizar.es/add/*"]
    }],
    "permissions": [
        "activeTab",
        "tabs"
    ]
}
